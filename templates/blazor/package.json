{
  "name": "aspnet",
  "version": "1.0.0",
  "private": true,
  "instapack": {
    "output": "wwwroot",
    "namespace": "instapack",
    "alias": {
      "jquery": "jquery/dist/jquery.slim"
    },
    "copy": [
      {
        "library": "@fortawesome/fontawesome-free",
        "files": [
          "webfonts",
          "!webfonts/fa-brands*"
        ],
        "destination": "webfonts"
      }
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@types/jquery": "^3.3.38",
    "@types/luxon": "^1.22.1",
    "@types/requirejs": "^2.1.31",
    "bootstrap": "^4.4.1",
    "jquery": "^3.5.1",
    "linq": "^3.2.2",
    "luxon": "^1.24.1",
    "popper.js": "^1.16.1",
    "sweetalert2": "^9.10.12",
    "ts-polyfill": "^3.8.2",
    "tslib": "^1.11.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "eslint": "^6.8.0",
    "typescript": "^3.8.3"
  }
}
